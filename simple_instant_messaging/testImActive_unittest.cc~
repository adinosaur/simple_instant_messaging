#include "imActiveUsers.h"
#include "gtest/gtest.h"

using namespace std;

TEST(IMActiveUsersTest, Add) {
  ImActiveUsers im_active_users;
  EXPECT_EQ(im_active_users.add("user1"), true);
  EXPECT_EQ(im_active_users.add("user2"), true);
  EXPECT_EQ(im_active_users.add("user3"), true);
  EXPECT_EQ(im_active_users.add("user1"), false);
  EXPECT_EQ(im_active_users.add("user2"), false);
  EXPECT_EQ(im_active_users.size(), 3);
}

void test() {
    ImActiveUsers im_active_users;

    im_active_users.add("user1");
    im_active_users.add("user2");
    im_active_users.add("user3");
    
    im_active_users.print(cout);
}

int main() 
{
    test();
    return 0;
}
